<?xml version="1.0"?>
<extension key="de.theanimalsociety.bbapi" type="module">
  <file>bbapi</file>
  <name>Bus Bridge API</name>
  <description>Custom Extension for adding new contacts over API.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>info@systopia.de</email>
  </maintainer>
  <urls>
