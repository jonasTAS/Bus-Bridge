    <url desc="Documentation">https://github.com/jonasTAS/Bus-Bridge/blob/master/README.md</url>
    <url desc="Support">https://github.com/jonasTAS/Bus-Bridge/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
